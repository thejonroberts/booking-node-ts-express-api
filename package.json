{
  "name": "booking-node-api",
  "version": "1.0.0",
  "description": "a quick api to run some experiments with",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejonroberts/booking-node-api"
  },
  "engines": {
    "node": "10.2.1"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha test/server.test.js -R spec",
    "dev-test": "mocha --watch --recursive -R spec",
    "faker": "node data/generation/generate-json-data.js",
    "teardown": "sequelize db:migrate:undo:all",
    "build": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "dbrb": "npm run teardown && npm run build && npm run seed",
    "dev": "nodemon server.js",
    "start": "node server.js",
    "ci-db":
      "./node_modules/sequelize-cli/bin/sequelize db:migrate && ./node_modules/sequelize-cli/bin/sequelize db:seed:all"
  },
  "author": "Jon Roberts",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-sonarjs": "^0.1.0",
    "faker": "^4.1.0",
    "markdownlint": "^0.10.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  }
}

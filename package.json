{
  "name": "booking-node-api",
  "version": "1.0.0",
  "description": "a quick api to run some experiments with",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejonroberts/booking-node-api"
  },
  "main": "index.js",
  "scripts": {
		"test": "mocha --watch --recursive -R spec",
		"teardown": "sequelize db:migrate:undo:all",
		"build": "sequelize db:migrate",
		"seed": "sequelize db:seed:all",
		"dbrb": "npm run teardown && npm run build && npm run seed",
    "dev": "nodemon server.js"
  },
  "author": "Jon Roberts",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.5"
  }
}

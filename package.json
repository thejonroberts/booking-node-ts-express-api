{
  "name": "booking-node-api",
  "author": "Jon Roberts",
  "license": "ISC",
  "version": "1.0.0",
  "description": "a quick api to run some experiments with",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejonroberts/booking-node-api"
  },
  "engines": {
    "node": "10.2.1",
    "npm": "6.1.0"
  },
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node ./dist/server.js",
    "build": "./node_modules/.bin/tsc",
    "lint": "node_modules/.bin/tslint --project . --fix -t stylish",
    "watch": "concurrently -k -p \"{name}:\" -n \"TS,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "ts-node": "./node_modules/mocha/bin/mocha -r ts-node/register --watch \"test/**/*.ts\"",
    "watch-test": "mocha --watch --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'",
    "pretest": "npm run build",
    "test": "nyc --reporter=html --reporter=text mocha --compilers ts:ts-node/register --recursive test/**/*.test.ts --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "teardown": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "reset": "npm run faker && npm run teardown && npm run migrate && npm run seed",
    "faker": "node db/data/generation/generate-json-data.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/mocha": "^5.2.3",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.4.9",
    "@types/sequelize": "^4.27.21",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "coveralls": "^3.0.1",
    "faker": "^4.1.0",
    "husky": "^1.0.0-rc.9",
    "markdownlint": "^0.10.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0",
    "ts-node": "^6.1.2",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-sonarts": "^1.7.0",
    "typescript": "^2.9.2"
  }
}
